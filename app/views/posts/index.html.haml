- @posts.each do |post|
  .container
    .post
      .post_header

        .dislike_count
          =post.dislike_count
          dislikes
        .user_id
          =post.user_id

      .photo
        =image_tag post.photo, size: "400x400"
      .description
        =post.description
    .commands
      =link_to 'Show',post
      =link_to 'dislike', dislike_post_path(post)

