<!DOCTYPE html>
<html>
<head>
  <title>Instagram</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="sidebar">
  <div id="top_side">
    <div id="photo">
    </div>
    <div id="about">
      Roof party Banksy Brooklyn tofu small
    </div>
  </div>
  <div id="bot_side">
    <ul>
      <li><%=link_to "New pic", new_post_path %></li>
      <li><%=link_to "Log out", new_post_path %></li>
    </ul>
  </div>
</div>

<div id="wrap">
    <div id="header">
        <div id="authentication">
            <%=link_to "Log In", new_post_path, class:'authenticate' %>
            <%=link_to "Sign Up", new_post_path, class:'authenticate'%>
        </div>
    </div>
    <%= yield %>
</div>
</body>
</html>
